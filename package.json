{
  "name": "wrdlnkdn",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "--- BOOT SEQUENCE (Environment Optimization) ---": "",
    "dev": "concurrently -n \"VITE,SUPA,API,HEALTH\" -c \"blue,green,magenta,cyan\" \"npm run vite\" \"npm run supabase:start\" \"npm run api\" \"npm run health:launch\"",
    "health:launch": "bash ./scripts/health-check.sh",
    "launch": "npm run health:launch",
    "vite": "vite",
    "api": "tsx watch backend/server.ts",
    "--- SYSTEM AUDITS (Verification Layer) ---": "",
    "precheck": "bash ./scripts/precheck.sh",
    "check": "npm run typecheck && npm run lint && npm run format:check && npm run test:run",
    "check:full": "npm run check && npm run test:e2e",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --cache --fix",
    "lint:md": "markdownlint '**/*.md' --ignore node_modules",
    "lint:md:fix": "markdownlint '**/*.md' --fix --ignore node_modules",
    "typecheck": "tsc --noEmit --pretty false",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "--- TESTING (Behavioral Integrity) ---": "",
    "test": "vitest",
    "test:run": "vitest run",
    "test:rls": "vitest run tests/rls",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "--- DATABASE (Physical Layer) ---": "",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:reset": "npx supabase db reset",
    "supabase:gen-types": "npx supabase gen types typescript --local | node scripts/gen-supabase-types.mjs",
    "--- BUILD & DEPLOY (Legacy Code Prevention) ---": "",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.md": [
      "prettier --write",
      "markdownlint --fix"
    ],
    "*.{json,css,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@fortawesome/fontawesome-svg-core": "7.1.0",
    "@fortawesome/free-solid-svg-icons": "7.1.0",
    "@fortawesome/react-fontawesome": "3.1.0",
    "@mui/icons-material": "7.3.5",
    "@mui/material": "7.3.5",
    "@supabase/supabase-js": "2.89.0",
    "cors": "2.8.5",
    "express": "5.2.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-router-dom": "7.12.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.11.0",
    "@eslint/js": "9.17.0",
    "@playwright/test": "1.57.0",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.6",
    "@types/node": "22.10.5",
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2",
    "@typescript-eslint/eslint-plugin": "8.18.2",
    "@typescript-eslint/parser": "8.18.2",
    "@vitejs/plugin-react": "4.3.4",
    "concurrently": "9.2.1",
    "dotenv": "17.2.3",
    "eslint": "9.39.2",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-playwright": "2.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "0.4.16",
    "globals": "15.14.0",
    "husky": "9.1.7",
    "lint-staged": "15.2.11",
    "markdownlint-cli": "0.47.0",
    "prettier": "3.4.2",
    "tsx": "4.20.5",
    "typescript": "5.7.2",
    "typescript-eslint": "8.18.2",
    "vite": "6.4.1",
    "vitest": "4.0.16"
  }
}
